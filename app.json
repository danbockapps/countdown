{
  "expo": {
    "name": "Countdown",
    "slug": "countdown",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios"
    ],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/calendar.png",
    "splash": {
      "image": "./assets/annie-spratt5.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIUserInterfaceStyle": "Light"
      },
      "bundleIdentifier": "net.danbock.countdown"
    }
  }
}
